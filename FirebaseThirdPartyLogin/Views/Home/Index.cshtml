@{
    ViewData["Title"] = "Home Page";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
</head>

<body>
    <div id="title">
        <transition name="fade">
            <h1>{{text}}</h1>
        </transition>
        <p><p>
        <button v-on:click="reverse">reverse</button>
        <p><p>
        <input v-model="text">
    </div>

    <p><p>
    <script src="~/js/logout.js" asp-append-version="true"></script>
    <script>
        var setTitle = new Vue({
            el: '#title',
            data:
            {
                text: 'ASP NET Core',
            },
            methods:
            {
                reverse: function () {
                    this.text = this.text.split('').reverse().join('')
                }
            }
        });
    </script>
</body>
</html>